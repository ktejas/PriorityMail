if(inst != noone &amp;&amp; inst.state == "readyForPickUp" &amp;&amp; inst.Lane == currentLane )
{
inst.state = "PowerUp";
return 0;
}

inst = instance_nearest(x,y,mailBoxGreen_obj)
if(inst != noone &amp;&amp; inst.state == "readyForPickUp" &amp;&amp; inst.Lane == currentLane)
{
inst.state = "PowerUp";
return 0;
}
var timeElapsed = current_time - currentPowerUpTime;
if(timeElapsed &gt; powerUpDuration )
{
state = "idle";
prioryTruckNumber ="Red";
}

}
